@model Dictionary<string, List<string>>

@{
    ViewData["Title"] = "Результаты фильтрации";
}

<h2>Результаты фильтрации</h2>
<ul>
    @foreach (var filter in Model)
    {
        <li><strong>@filter.Key:</strong> @string.Join(", ", filter.Value)</li>
    }
</ul>
<a href="/FileUpload/Index">Назад</a>
